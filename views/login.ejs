<% include navBar.html %>
<html>
  <body>
    <section class="cta1">
      <div class="cta-content">
        <div class="container px-5">
          <h2 class="display-1 lh-1 mb-4">
            <%= __('Sign in to') %> <%= title %>
          </h2>
          <form method="POST">
            <div class="mb-3">
              <label for="email" class="form-label"
                ><%= __('Email address') %>
              </label>
              <input
                name="email"
                type="email"
                class="form-control"
                id="email"
                aria-describedby="emailHelp"
              />
            </div>
            <div class="mb-3">
              <label for="password" class="form-label"
                ><%= __('Password') %>
              </label>
              <input
                name="password"
                type="password"
                class="form-control"
                id="password"
              />
            </div>
            <button type="submit" class="btn btn-primary">
              <%= __('Submit') %>
            </button>
            <div><%= error %></div>
          </form>
        </div>
      </div>
    </section>
  </body>
</html>
